<requests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_AdobeStockAsset:etc/adobe_stock_requests.xsd">
    <request name="adobe_stock_image_search">
        <filters>
            <filter name="words" field="words"/>
            <filter name="content_type_filter" field="content_type"/>
            <filter name="offensive_filter" field="offensive"/>
            <filter name="isolated_filter" field="isolated"/>
            <filter name="orientation_filter" field="orientation"/>
            <filter name="colors_filter" field="colors"/>
            <filter name="premium_filter" field="premium"/>
        </filters>
        <resultColumns>
            <column name="nb_results" field="NB_RESULTS"/>
            <column name="id" field="ID"/>
            <column name="COMP_URL" field="COMP_URL"/>
        </resultColumns>
        <from>0</from>
        <size>32</size>
        <locale>En_en</locale>
    </request>
</requests>
